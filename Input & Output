#Hello world
print("HELLO WORLD")


#Hello World with tab
print("Hello World \t Hello World")


#Hello World with new line
print("Hello World \nHello World")


#Integer Input
a=int(input())
print(a)


#Float Input
a=float(input())
print(a)


#Character Input
a=input()
print(a)


#String Input
a=input()
print(a)


#Ascii-I
a=input()
print(ord(a))


#Ascii-II
a=int(input())
print(chr(a))
