#Budget
A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
cost=(350.34*A)+(230.90*B)+(190.55*C)+(125.30*D)+(180.90*E)
if(cost<15000):
    print("yes")
else:
    print("no")
    
    
#Sece Dining
E=input()
R=int(input())
if(E=="front"):
    if(R==1):
        print("Left Handed")
    elif(R==2):
        print("Right Handed")
    else:
        print("Invalid")
elif(E=="rear"):
    if(R==1):
        print("Right Handed")
    elif(R==2):
        print("Left Handed")
    else:
        print("Invalid")
else:
    print("Invalid Input")
    
    
#Intersection of a Circle
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
C1C2 = ((x1-x2)**2 + (y1-y2)**2)**0.5
if(C1C2==r1+r2):
    print("The circles are tangential")
elif(C1C2<r1+r2):
    print("The circles overlap")
else:
    print("The circles do not overlap")
    
    
#Microwave Oven
I=int(input("Enter the number of items:\n"))
H=float(input("Enter the single item heating time:\n"))
if(I==2):
    P=((50/100)*H)+H
    print("The recommended heating time is","%.2f"%P)
elif(I==3):
    Q=2*H
    print("The recommended heating time is","%.2f"%Q)
else:
    print("Number of items is more")
    
    
